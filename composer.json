{
    "name": "creode/laravel-nova-blog",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Creode\\LaravelNovaBlog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creode\\LaravelNovaBlog\\ToolServiceProvider",
                "Creode\\LaravelNovaBlog\\LaravelNovaBlogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
